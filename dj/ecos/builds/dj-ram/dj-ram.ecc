# of resource which a BOOTP solution does not require.
#
cdl_component CYGPKG_NET_DHCP {
    # ActiveIf constraint: (CYGHWR_NET_DRIVERS > 0)
    #     CYGHWR_NET_DRIVERS == 1
    #   --> 1

